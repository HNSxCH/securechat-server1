{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    { "src": "/pubkeys/(?<roomId>[^/]+)/(?<userId>[^/]+)", "dest": "/api/pubkeys/[roomId]/[userId]" },
    { "src": "/roomkeys/(?<roomId>[^/]+)", "dest": "/api/roomkeys/[roomId]" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ]
}
